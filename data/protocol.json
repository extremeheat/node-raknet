          "switch",
          {
            "compareTo":"reliability",
            "fields":{
              "1":"i8",
              "3":"i8",
              "4":"i8"
            },
            "default":"void"
          }
        ]
      },
      {
        "name":"splitCount",
        "type":[
          "switch",
          {
            "compareTo":"hasSplit",
            "fields":{
              "16":"i32",
              "0":"void"
            }
          }
        ]
      },
